[core]
  quotepath = false
  editor = vim
  excludesfile = ~/.gitignore_global
  preloadindex = true
  fscashe = true
  autoCRLF = false
[include]
  path = ~/.gitconfig.local
[color]
  ui = auto
  diff = auto
  status = auto
  branch = auto
[alias]
  clone-shallow = clone --depth=1
  hist = log --pretty=format:\"%C(magenta)%h%Creset %C(cyan)%ad%Creset - %s %C(dim)<%C(italic)%an%Creset%C(dim)>%Creset%C(yellow)%d%Creset\" --graph --date=short
  ffeature = flow feature
  fstart = flow feature start
  ffinish = flow feature finish
[merge]
  ff = false
[diff]
  tool = vimdiff
[push]
  default = simple
[pull]
  rebase = false
  ff = true
[credential]
  helper = store
